###  http://www.example.com/linkedGDDS#Axis

:Axis rdf:type owl:Class ;
      
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :usesReferenceSystem ;
                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :ReferenceSystem
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :axisNum ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange xsd:positiveInteger
                      ] ;
      
      rdfs:comment "A method for indexing data" .



###  http://www.example.com/linkedGDDS#CategoricalAxis

:CategoricalAxis rdf:type owl:Class ;
                 
                 rdfs:subClassOf :Axis ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :axisValues ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onDataRange rdf:PlainLiteral
                                 ] ;
                 
                 owl:disjointWith :RangeAxis ;
                 
                 rdfs:comment "A coordinate defined by a set of allowable values e.g. \"station 1, station 2, station 3\"" .



###  http://www.example.com/linkedGDDS#Coverage

:Coverage rdf:type owl:Class ;
          
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasDomain ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Domain
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasRange ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Range
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasParameter ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Parameter
                          ] ;
          
          rdfs:comment "A single, coherent chunk of data" .



###  http://www.example.com/linkedGDDS#CoverageCollection

:CoverageCollection rdf:type owl:Class ;
                    
                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                      owl:onProperty :containsCoverage ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :Coverage
                                    ] ;
                    
                    rdfs:comment "A collection of related coverages defined in a single location" .



###  http://www.example.com/linkedGDDS#Domain

:Domain rdf:type owl:Class ;
        
        rdfs:subClassOf [ rdf:type owl:Class ;
                          owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :hasAxis ;
                                                                 owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                                 owl:onClass :Axis
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :rangeAxisOrder ;
                                                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                 owl:onDataRange rdf:PlainLiteral
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ]
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :hasAxis ;
                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass :Axis
                                        ]
                                      )
                        ] ;
        
        rdfs:comment "Specifies the coordinate system for a coverage" .



###  http://www.example.com/linkedGDDS#GridDomain

:GridDomain rdf:type owl:Class ;
            
            rdfs:subClassOf :Domain ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasAxis ;
                              owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                              owl:onClass :SpatialAxis
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasAxis ;
                              owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                              owl:onClass [ rdf:type owl:Class ;
                                            owl:complementOf [ rdf:type owl:Class ;
                                                               owl:unionOf ( :SpatialAxis
                                                                             :TemporalAxis
                                                                           )
                                                             ]
                                          ]
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasAxis ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :TemporalAxis
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasAxis ;
                              owl:maxQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                              owl:onClass :SpatialAxis
                            ] ;
            
            rdfs:comment "Most basic kind of domain. Represents a 2D, 3D or 4D grid of values. Requires 2-3 spatial axes and 0-1 temporal axes" .



